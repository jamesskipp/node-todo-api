{
    "extends": "airbnb",
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "rules": {
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "consistent-return": 0,
      "no-console": 0
    }
}
